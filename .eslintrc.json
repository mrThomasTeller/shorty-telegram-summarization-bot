{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "import/extensions": 0
  }
}
